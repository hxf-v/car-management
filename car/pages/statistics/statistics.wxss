    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
    font-size: 40rpx
}

.group{
  margin-left: 20rpx;
}
.itemContent {
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  border-top: 1px solid #e9eaec;
  font-size: 28rpx;
  /* padding-top: 30rpx; */
  color: #495060;
}
.itemContent-item {
  display: flex;
  width: 100%;
  height: 20%;
  flex: 0 0 20%;
}
.itemContent-item-title {
  color: #2d8cf0;
  font-weight: bold;
  flex: 0 0 200rpx;
  height: 100%
}
.listWW{
  width: 100%;
  overflow: hidden
}
.itemContent-item-content{
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
